{
    "name": "acsystems/keycloak-php-sdk",
    "description": "PHP wrapper for the KeyCloak admin API",
    "keywords": [
        "keycloak",
        "sdk",
        "admin",
        "identity server"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "WebDev",
            "email": "web.support@ac-systems.com",
            "homepage": "https://ac-systems.be"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.6",
        "league/oauth2-client": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "roave/security-advisories": "dev-latest",
        "symfony/dotenv": "^4.4"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php --testdox tests"
    },
    "autoload": {
        "psr-4": {
            "Keycloak\\": "src/",
            "App\\Tests\\": "tests"
        }
    }
}
